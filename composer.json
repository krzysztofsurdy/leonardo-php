{
    "name": "krzysztofsurdy/leonardo-php",
    "description": "Leonardo.ai SDK for PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LeonardoAI\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Krzysztof Surdy",
            "email": "surdykrzysztof@gmail.com"
        }
    ],
    "require-dev": {
        "jane-php/open-api-3": "^7.8",
        "friendsofphp/php-cs-fixer": "^3.59"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "php": "^8.2",
        "jane-php/open-api-runtime": "^7.8"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
